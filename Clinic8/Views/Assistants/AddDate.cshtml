@model Clinic8.Models.Dates

@{
    ViewData["Title"] = "AddDate";
}



<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="date_date" class="control-label"></label>
                <input asp-for="date_date" class="form-control" />
                <span asp-validation-for="date_date" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



